<p id="notice"><%= notice %></p>

<h1>System Names</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @system_names.each do |system_name| %>
      <tr>
        <td><%= system_name.content %></td>
        <td><%= system_name.user_id %></td>
        <td><%= link_to 'Show', system_name %></td>
        <td><%= link_to 'Edit', edit_system_name_path(system_name) %></td>
        <td><%= link_to 'Destroy', system_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New System Name', new_system_name_path %>
