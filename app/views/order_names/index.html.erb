<p id="notice"><%= notice %></p>

<h1>Order Names</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th>System name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_names.each do |order_name| %>
      <tr>
        <td><%= order_name.content %></td>
        <td><%= order_name.user_id %></td>
        <td><%= order_name.system_name_id %></td>
        <td><%= link_to 'Show', order_name %></td>
        <td><%= link_to 'Edit', edit_order_name_path(order_name) %></td>
        <td><%= link_to 'Destroy', order_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Name', new_order_name_path %>
